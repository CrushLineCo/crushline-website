{
  "name": "amplify-vite-react-template",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "predeploy": "bash scripts/check_amplify_tags.sh",
    "deploy": "npm run predeploy && npx ampx deploy",
    "use:sandbox": "AWS_PROFILE=crushline AWS_REGION=us-east-2 npx ampx sandbox --identifier codespace --once && mv -f amplify_outputs.json amplify_outputs.sandbox.json && jq -r '.data.url' amplify_outputs.sandbox.json || true",
    "use:prod": "AWS_PROFILE=crushline AWS_REGION=us-east-2 npx ampx generate outputs --app-id d3lbqnud4lc08c --branch main --out-dir . && mv -f amplify_outputs.json amplify_outputs.prod.json && jq -r '.data.url' amplify_outputs.prod.json || true"
  },
  "dependencies": {
    "@aws-amplify/ui-react": "^6.11.2",
    "aws-amplify": "^6.15.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^7.8.0"
  },
  "devDependencies": {
    "@aws-amplify/backend": "^1.16.1",
    "@aws-amplify/backend-cli": "^1.8.0",
    "@tailwindcss/vite": "^4.1.11",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.21",
    "aws-cdk": "^2.138.0",
    "aws-cdk-lib": "^2.138.0",
    "constructs": "^10.3.0",
    "esbuild": "^0.20.2",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.11",
    "tsx": "^4.7.2",
    "typescript": "^5.4.5",
    "vite": "^5.4.10"
  }
}
